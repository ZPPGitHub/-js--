	function xuanfw(obj,ojk,num,fnEnd){
		for(var i=0;i<obj.length;i++){
			obj[i].aaa = true
			obj[i].index = i
			obj[i].onclick = function(){
				if(this.aaa === true){
					for(var i=0;i<obj.length;i++){
						obj[i].style.color = "#666"
						ojk[i].style.backgroundPosition = "-16px 0px"
						obj[i].aaa = true
						//eee = []
