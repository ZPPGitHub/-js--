					ybService[num].firstElementChild.nextElementSibling.nextElementSibling.innerText = obj[this.index].childNodes[1].lastElementChild.innerText
					ppp.push(obj[this.index].childNodes[1].lastElementChild.innerText)
				}else{
					this.style.color = "#666"
					ojk[this.index].style.backgroundPosition = "-16px 0px"
					this.aaa = true
					ybService[num].classList.remove("poliu")
					moreService[num].style.display = "none"
					var ind = ppp.indexOf(obj[this.index].childNodes[1].lastElementChild.innerText)
					ppp.splice(ind,1)	//然后删除数组中当前位置的字符串
				}
				if(fnEnd)fnEnd()
			}
		}
